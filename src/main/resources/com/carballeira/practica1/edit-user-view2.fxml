<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@css/editUser2Styles.css" fx:controller="com.carballeira.practica1.controller.EditUser2Controller">
    <children>
        <Label id="main-title" alignment="CENTER" layoutX="122.0" layoutY="22.0" text="EDITAR USUARIO" />

        <Label layoutX="20.0" layoutY="115.0" text="Nombre" />
        <TextField fx:id="nombre" layoutX="100.0" layoutY="115.0" />

        <Label layoutX="20.0" layoutY="155.0" text="Email" />
        <TextField fx:id="email" layoutX="100.0" layoutY="155.0" />

        <Label layoutX="20.0" layoutY="195.0" text="Telefono" />
        <TextField fx:id="telefono" layoutX="100.0" layoutY="195.0" />

        <Button fx:id="editarUsuario" layoutX="15.0" layoutY="294.0" maxWidth="120.0" minHeight="-Infinity" minWidth="120.0" onAction="#editarUsuario" prefWidth="120.0" text="Editar Usuario" />
        <Button fx:id="botonVolver" layoutX="157.0" layoutY="294.0" maxWidth="120.0" minWidth="120.0" onAction="#botonVolverActionPerformed" prefWidth="82.0" text="Volver" />
      <Label layoutX="21.0" layoutY="232.0" text="Admin?" />
      <CheckBox fx:id="adminCB" layoutX="100.0" layoutY="232.0" mnemonicParsing="false" />
    </children>
</AnchorPane>
